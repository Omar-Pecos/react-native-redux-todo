{
  "expo": {
    "name": "ToDox",
    "slug": "react-native-redux-todo",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/todox_icon.png",
    "splash": {
      "image": "./assets/todox_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f1f0eb"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/todox_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.omarpv.todox"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
